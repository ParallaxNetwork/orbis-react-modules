{
  "name": "orbis-react-modules",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "test"
    ]
  },
  "scripts": {
    "dev": "npm --prefix ./test/ run dev",
    "build": "npm package-install build-package test-install build-test",
    "package-install": "npm  --prefix ./packages/orbis-react-modules/ npm install",
    "test-install": "npm  --prefix ./test/ npm install",
    "build-package": "npm --prefix ./packages/orbis-react-modules/ run build",
    "build-test": "npm --prefix ./test/ run build",
    "preview": "npm --prefix ./test/ run preview"
  }
}
